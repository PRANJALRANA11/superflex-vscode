{
  "name": "superflex",
  "publisher": "aquilalabs",
  "displayName": "Superflex: Your Frontend AI Pair Programmer",
  "description": "Never code alone again. Superflex is your AI frontend pair programmer that helps you code faster and better.",
  "icon": "small_logo.png",
  "version": "0.0.1",
  "author": "Riphal <https://twitter.com/_riphal_>",
  "license": "MIT",
  "bugs": {
    "email": "boris@superflex.ai"
  },
  "categories": [
    "AI",
    "Chat",
    "Snippets",
    "Visualization",
    "Machine Learning",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "ai",
    "css",
    "html",
    "chat",
    "cody",
    "react",
    "figma",
    "sketch",
    "copilot",
    "tabnine",
    "refactor",
    "tailwind",
    "superflex",
    "components",
    "javascript",
    "typescript",
    "screenshot",
    "autocomplete",
    "ui components",
    "image to code",
    "figma to code",
    "sketch to code",
    "design to code",
    "frontend tools",
    "github copilot",
    "code completion",
    "code generation",
    "screenshot to code",
    "generate components"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "pretest": "npm run build && npm run lint",
    "test": "vscode-test",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "package": "npm run check-types && node esbuild.js --production",
    "vscode:prepublish": "npm --prefix webview-ui/ run build && npm run package",
    "vsce:package": "vsce package --no-dependencies --no-yarn",
    "vsce:publish": "vsce publish"
  },
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "superflex.chat.new-thread",
          "group": "navigation",
          "when": "view == superflex.chat && superflex.chat.authenticated"
        },
        {
          "command": "superflex.project.sync",
          "group": "navigation",
          "when": "view == superflex.chat && superflex.chat.authenticated"
        },
        {
          "command": "superflex.auth.signout",
          "group": "navigation",
          "when": "view == superflex.chat && superflex.chat.authenticated"
        }
      ]
    },
    "submenus": [
      {
        "id": "superflex.chat.submenu",
        "label": "Superflex"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "superflex",
          "title": "Superflex",
          "icon": "small_logo.png"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "superflex.loading",
        "contents": "Loading please wait..."
      }
    ],
    "views": {
      "superflex": [
        {
          "type": "webview",
          "id": "superflex.chat",
          "name": "Chat",
          "when": "superflex.chat.webview == 'chat'"
        },
        {
          "id": "superflex.loading",
          "name": "Loading",
          "when": "superflex.chat.webview == 'loading'"
        }
      ]
    },
    "commands": [
      {
        "command": "superflex.auth.signin",
        "category": "Superflex",
        "title": "Sign In"
      },
      {
        "command": "superflex.auth.signout",
        "category": "Superflex",
        "title": "Sign Out",
        "icon": "$(sign-out)"
      },
      {
        "command": "superflex.figma.connect",
        "category": "Superflex",
        "title": "Connect Figma Account"
      },
      {
        "command": "superflex.figma.disconnect",
        "category": "Superflex",
        "title": "Disconnect Figma Account"
      },
      {
        "command": "superflex.chat.focus-input",
        "category": "Superflex",
        "title": "Focus chat input"
      },
      {
        "command": "superflex.chat.new-thread",
        "category": "Superflex",
        "title": "New Chat Thread",
        "icon": "$(plus)"
      },
      {
        "command": "superflex.project.sync",
        "category": "Superflex",
        "title": "Project Sync",
        "icon": "$(refresh)"
      },
      {
        "command": "superflex.auth.remove-openai-api-key",
        "category": "Superflex",
        "title": "Remove OpenAI API Key"
      }
    ]
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.14.11",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.36",
    "async": "^3.2.5",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.7",
    "crypto": "^1.0.1",
    "fast-glob": "^3.3.2",
    "openai": "^4.52.1",
    "uuidv4": "^6.2.13"
  }
}
