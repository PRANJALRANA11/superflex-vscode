  "engines": {
    "vscode": "^1.70.0",
    "node": ">=20.11.0"
  },
  "engine-strict": true,
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "pretest": "npm run build && npm run lint",
    "test": "vscode-test",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint:webview-ui": "eslint webview-ui/**/*.{ts,tsx}",
    "package": "npm run check-types && node esbuild.js --production",
    "vscode:prepublish": "npm --prefix webview-ui/ run build && npm run package",
    "vsce:package": "vsce package --no-dependencies --no-yarn",
    "vsce:publish": "vsce publish"
  },
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "superflex.chat.new-thread",
          "group": "navigation",
          "when": "view == superflex.chat && superflex.chat.authenticated"
        },
        {
          "command": "superflex.project.sync",
          "group": "navigation",
          "when": "view == superflex.chat && superflex.chat.authenticated"
        },
        {
          "command": "superflex.auth.signout",
          "group": "navigation",
          "when": "view == superflex.chat && superflex.chat.authenticated"
        }
      ]
    },
    "submenus": [
      {
        "id": "superflex.chat.submenu",
        "label": "Superflex"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "superflex",
          "title": "Superflex",
          "icon": "small_logo.png"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "superflex.loading",
        "contents": "Loading please wait..."
      }
    ],
    "views": {
      "superflex": [
        {
          "type": "webview",
          "id": "superflex.chat",
          "name": "Chat",
          "when": "superflex.chat.webview == 'chat'"
        },
        {
          "id": "superflex.loading",
          "name": "Loading",
          "when": "superflex.chat.webview == 'loading'"
        }
      ]
    },
    "commands": [
      {
        "command": "superflex.auth.signin",
        "category": "Superflex",
        "title": "Sign In"
      },
      {
        "command": "superflex.auth.signout",
        "category": "Superflex",
        "title": "Sign Out",
        "icon": "$(sign-out)"
      },
      {
        "command": "superflex.figma.connect",
        "category": "Superflex",
        "title": "Connect Figma Account"
      },
      {
        "command": "superflex.figma.disconnect",
        "category": "Superflex",
        "title": "Disconnect Figma Account"
      },
      {
        "command": "superflex.chat.focus-input",
        "category": "Superflex",
        "title": "Focus chat input"
      },
      {
        "command": "superflex.chat.new-thread",
        "category": "Superflex",
        "title": "New Chat Thread",
        "icon": "$(plus)"
      },
      {
        "command": "superflex.project.sync",
        "category": "Superflex",
        "title": "Project Sync",
        "icon": "$(refresh)"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "superflex.analytics": {
          "type": "boolean",
          "default": true,
          "description": "Specify whether to allow Superflex to collect product usage analytics"
        }
      }
    },
    "keybindings": [
      {
        "command": "superflex.chat.focus-input",
        "key": "cmd+;",
        "mac": "cmd+;",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.11.0",
    "@types/vscode": "1.70",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.23.0",
    "eslint": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.36",
    "async": "^3.2.5",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.7",
    "crypto": "^1.0.1",
    "fast-glob": "^3.3.2",
    "node-machine-id": "^1.1.12",
    "posthog-node": "^4.2.0",
    "uuidv4": "^6.2.13"
  }
}
